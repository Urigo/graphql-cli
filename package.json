{
  "name": "graphql-cli",
  "bin": {
    "graphql": "dist/bin.js",
    "gql": "dist/bin.js"
  },
  "version": "1.1.2",
  "description": "GraphQL CLI",
  "files": [
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc",
    "test": "npm run lint && tsc && ava tests/**/test.js --serial",
    "lint": "tslint {src,test}/**/*.ts",
    "start": "ts-node src/bin.ts"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphcool/graphql-cli.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphcool/graphql-cli/issues"
  },
  "homepage": "https://github.com/graphcool/graphql-cli#readme",
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/graphql": "^0.11.6",
    "@types/inquirer": "^0.0.35",
    "@types/lodash": "^4.14.85",
    "@types/node": "^8.0.52",
    "@types/ora": "^1.3.0",
    "@types/yargs": "^8.0.1",
    "ava": "^0.23.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "chalk": "^2.3.0",
    "disparity": "^2.0.0",
    "express": "^4.16.2",
    "express-request-proxy": "^2.0.0",
    "graphql": "^0.11.7",
    "graphql-config": "^1.0.7",
    "graphql-playground-middleware-express": "^1.1.4",
    "graphql-schema-linter": "^0.0.18",
    "inquirer": "^3.2.0",
    "js-yaml": "^3.9.0",
    "lodash": "^4.17.4",
    "npm-paths": "^0.1.3",
    "opn": "^5.1.0",
    "ora": "^1.3.0",
    "yargs": "^9.0.1"
  }
}
