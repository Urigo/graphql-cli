{
  "name": "graphql-cli",
  "version": "0.0.0-semantic-release",
  "main": "dist/index.js",
  "bin": {
    "graphql": "dist/bin.js",
    "gql": "dist/bin.js"
  },
  "typings": "./dist/index.d.ts",
  "description": "GraphQL CLI",
  "files": [
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "rm -rf dist && tsc -d",
    "pretest": "npm run build && npm run lint",
    "test": "ava --serial",
    "prepack": "npm run build",
    "pack": "pkg dist/bin.js -c package.json -o packed/graphql --options no-warnings",
    "lint": "tslint {src,test}/**/*.ts",
    "start": "ts-node src/bin.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-cli/graphql-cli.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphql-cli/graphql-cli/issues"
  },
  "homepage": "https://github.com/graphql-cli/graphql-cli#readme",
  "pkg": {
    "scripts": "dist/**/*.js",
    "targets": [
      "node9-alpine-x64",
      "node9-linux-x64",
      "node9-macos-x64",
      "node9-win-x64"
    ]
  },
  "release": {
    "publish": [
      "@semantic-release/npm",
      {
        "path": "@semantic-release/github",
        "assets": "packed/*"
      }
    ]
  },
  "devDependencies": {
    "@types/express": "4.11.1",
    "@types/graphql": "0.12.4",
    "@types/inquirer": "0.0.36",
    "@types/lodash": "4.14.104",
    "@types/node": "9.4.6",
    "@types/node-fetch": "1.6.7",
    "@types/ora": "1.3.1",
    "@types/request": "2.47.0",
    "@types/rimraf": "2.0.2",
    "@types/yargs": "11.0.0",
    "ava": "0.25.0",
    "pkg": "4.3.0",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "tslint-config-standard": "7.0.0",
    "typescript": "2.7.2"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "chalk": "^2.3.1",
    "command-exists": "^1.2.2",
    "cross-spawn": "^6.0.4",
    "disparity": "^2.0.0",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "express-request-proxy": "^2.0.0",
    "graphql": "^0.11.0 || ^0.12.0 || ^0.13.0",
    "graphql-cli-prepare": "1.4.19",
    "graphql-config": "2.0.1",
    "graphql-config-extension-graphcool": "1.0.8",
    "graphql-config-extension-openapi": "1.0.6",
    "graphql-config-extension-prisma": "0.0.9",
    "graphql-playground-middleware-express": "1.5.7",
    "graphql-schema-linter": "0.1.1",
    "inquirer": "5.1.0",
    "is-url-superb": "2.0.0",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.5",
    "node-fetch": "^2.0.0",
    "npm-paths": "^1.0.0",
    "opn": "^5.2.0",
    "ora": "^1.4.0",
    "parse-github-url": "^1.0.2",
    "request": "^2.83.0",
    "rimraf": "2.6.2",
    "source-map-support": "^0.5.3",
    "update-notifier": "^2.3.0",
    "yargs": "11.0.0"
  }
}
